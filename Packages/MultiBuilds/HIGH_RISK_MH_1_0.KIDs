"DATA",811.8,163,100,32,0)
PXRMBMI ; SLC/PKR - National BMI and BSA computed finding. ;09/01/2009
"DATA",811.8,163,100,33,0)
 ;;2.0;CLINICAL REMINDERS;**12**;Feb 04, 2005
"DATA",811.8,163,100,34,0)
 ;================================
"DATA",811.8,163,100,35,0)
BMI(DFN,NGET,BDT,EDT,NFOUND,TEST,DATE,DATA,TEXT) ;Multi-occurrence computed
"DATA",811.8,163,100,36,0)
 ;finding for BMI.
"DATA",811.8,163,100,37,0)
 N BMI,HDATE,HT,IND,TDATE,WHL,WT
"DATA",811.8,163,100,38,0)
 ;Get the list of weight and height measurements.
"DATA",811.8,163,100,39,0)
 D WANDHL(DFN,NGET,BDT,EDT,.NFOUND,.WHL)
"DATA",811.8,163,100,40,0)
 F IND=1:1:NFOUND D
"DATA",811.8,163,100,41,0)
 . S TDATE=$P(WHL(IND),U,1),WT=$P(WHL(IND),U,2)
"DATA",811.8,163,100,42,0)
 . S HT=$P(WHL(IND),U,3),HDATE=$P(WHL(IND),U,4)
"DATA",811.8,163,100,43,0)
 . S TEST(IND)=1,DATE(IND)=TDATE
"DATA",811.8,163,100,44,0)
 . S TEXT(IND)="height measured "_$$EDATE^PXRMDATE(HDATE)
"DATA",811.8,163,100,45,0)
 . S BMI=WT/(HT*HT)
"DATA",811.8,163,100,46,0)
 . S BMI=$FN(BMI,"",1)
"DATA",811.8,163,100,47,0)
 . S (DATA(IND,"VALUE"),DATA(IND,"BMI"))=BMI
"DATA",811.8,163,100,48,0)
 Q
"DATA",811.8,163,100,49,0)
 ;
"DATA",811.8,163,100,50,0)
 ;================================
"DATA",811.8,163,100,51,0)
BSA(DFN,NGET,BDT,EDT,NFOUND,TEST,DATE,DATA,TEXT) ;Multi-occurrence computed
"DATA",811.8,163,100,52,0)
 ;finding for BSA. The coefficients have been adjusted for heights
"DATA",811.8,163,100,53,0)
 ;in cm and weights in kg expect for Boyd where the weight is grams.
"DATA",811.8,163,100,54,0)
 ;The default is to use the Mosteller formula.
"DATA",811.8,163,100,55,0)
 N BSA,FORMULA,HT,IND,TDATE,TYPE,WHL,WT
"DATA",811.8,163,100,56,0)
 S TYPE=$S(TEST="":"M",TEST="M":"M",TEST="D":"D",TEST="H":"H",TEST="G":"G",TEST="B":"B",1:"M")
"DATA",811.8,163,100,57,0)
 S FORMULA=$S(TYPE="M":"Mosteller",TYPE="B":"Boyd",TYPE="D":"DuBois and Dubois",TYPE="H":"Haycock",TYPE="G":"Gehan and George",1:"Mosteller")_" formula"
"DATA",811.8,163,100,58,0)
 ;Get the list of weight and height measurements.
"DATA",811.8,163,100,59,0)
 D WANDHL(DFN,NGET,BDT,EDT,.NFOUND,.WHL)
"DATA",811.8,163,100,60,0)
 F IND=1:1:NFOUND D
"DATA",811.8,163,100,61,0)
 . S TDATE=$P(WHL(IND),U,1),WT=$P(WHL(IND),U,2)
"DATA",811.8,163,100,62,0)
 . S HT=$P(WHL(IND),U,3),HDATE=$P(WHL(IND),U,4)
"DATA",811.8,163,100,63,0)
 . S TEST(IND)=1,DATE(IND)=TDATE
"DATA",811.8,163,100,64,0)
 . I TYPE="M" S BSA=$$SQRT^XLFMTH((WT*HT)/36)
"DATA",811.8,163,100,65,0)
 . I TYPE="D" S BSA=.20247*$$PWR^XLFMTH(HT,.725)*$$PWR^XLFMTH(WT,.425)
"DATA",811.8,163,100,66,0)
 . I TYPE="H" S BSA=.15058*$$PWR^XLFMTH(HT,.3964)*$$PWR^XLFMTH(WT,.5378)
"DATA",811.8,163,100,67,0)
 . I TYPE="G" S BSA=.164*$$PWR^XLFMTH(HT,.42246)*$$PWR^XLFMTH(WT,.51456)
"DATA",811.8,163,100,68,0)
 . I TYPE="B" D
"DATA",811.8,163,100,69,0)
 .. N WEXP
