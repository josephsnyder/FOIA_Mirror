 ;
"RTN","SD53P541",4,0)
 ; LOOP THROUGH PATIENT FILE (2), APPOINTMENT MULTIPLE
"RTN","SD53P541",5,0)
 ; PURPOSE OF VISIT FIELD, FOR ALL APPOINTMENTS THAT
"RTN","SD53P541",6,0)
 ; CONTAIN AN INVALID VALUE IN THAT FIELD AND CHANGE
"RTN","SD53P541",7,0)
 ; THE VALUE IN EACH OCCURRANCE TO A VALUE OF 3.
"RTN","SD53P541",8,0)
 ;
"RTN","SD53P541",9,0)
 ; ONLY VALID VALUES ARE:
"RTN","SD53P541",10,0)
 ;
"RTN","SD53P541",11,0)
 ; 2.98,9        PURPOSE OF VISIT       0;7 SET (Required)
"RTN","SD53P541",12,0)
 ;
"RTN","SD53P541",13,0)
 ;                       '1' FOR C&P;
"RTN","SD53P541",14,0)
 ;                       '2' FOR 10-10;
"RTN","SD53P541",15,0)
 ;                       '3' FOR SCHEDULED VISIT;
"RTN","SD53P541",16,0)
 ;                       '4' FOR UNSCHED. VISIT;
"RTN","SD53P541",17,0)
 ;
"RTN","SD53P541",18,0)
 ;
"RTN","SD53P541",19,0)
