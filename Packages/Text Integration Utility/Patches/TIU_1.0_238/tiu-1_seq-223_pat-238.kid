"RTN","TIULMED",6,0)
 ; provided default to 0 (with the exception of SUPPLIES).
"RTN","TIULMED",7,0)
 ;Required Parameters:
"RTN","TIULMED",8,0)
 ;  DFN       Patient identifier
"RTN","TIULMED",9,0)
 ;  TARGET    Where the medication data will be stored
"RTN","TIULMED",10,0)
 ;Optional Parameters:
"RTN","TIULMED",11,0)
 ;  ACTVONLY  0 - Active and recently expired meds
"RTN","TIULMED",12,0)
 ;            1 - Active meds only
"RTN","TIULMED",13,0)
 ;            2 - Recently expired meds only
"RTN","TIULMED",14,0)
